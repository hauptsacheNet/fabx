
# START OF Hn\Typo3Environment\Generator\HtaccessGenerator
# THIS AREA IS AUTO GENERATED BY hauptsache/typo3-environment
Options -MultiViews -Indexes
DirectoryIndex index.php
FallbackResource /index.php
SetEnv TYPO3_CONTEXT=Development
SetEnvIfNoCase Host \.hauptsache\.net$|test|alpha|beta|staging TYPO3_CONTEXT=Production/Staging
SetEnvIfNoCase Host ^www\. TYPO3_CONTEXT=Production
AddOutputFilterByType DEFLATE text/plain
AddOutputFilterByType DEFLATE text/html
AddOutputFilterByType DEFLATE text/css
AddOutputFilterByType DEFLATE text/csv
AddOutputFilterByType DEFLATE application/xml
AddOutputFilterByType DEFLATE application/xhtml+xml
AddOutputFilterByType DEFLATE application/rss+xml
AddOutputFilterByType DEFLATE application/atom+xml
AddOutputFilterByType DEFLATE application/javascript
AddOutputFilterByType DEFLATE application/x-shockwave-flash
AddOutputFilterByType DEFLATE application/json
AddOutputFilterByType DEFLATE image/svg+xml
AddOutputFilterByType DEFLATE font/ttf
AddOutputFilterByType DEFLATE font/woff
AddOutputFilterByType DEFLATE font/woff2
AddOutputFilterByType DEFLATE font/otf
Header merge Cache-Control 'public' 'expr=%{REQUEST_FILENAME} !~ /\.php$/'
Header always set X-Content-Type-Options 'nosniff'
Header always set X-UA-Compatible 'IE=edge' 'expr=%{CONTENT_TYPE} =~ m#^text/x?html#'
Header always set X-XSS-Protection '1; mode=block' 'expr=%{CONTENT_TYPE} =~ m#^text/x?html#'
Header always merge X-Frame-Options 'SAMEORIGIN' 'expr=%{CONTENT_TYPE} =~ m#^text/x?html#'
<If "%{HTTP_HOST} !~ /^www\./">
    # block most search engines when not on the live domain
    Require expr %{HTTP_USER_AGENT} !~ /aolbuild|baidu|bingbot|bingpreview|msnbot|duckduckgo|adsbot-google|googlebot|mediapartners-google|teoma|slurp|yandex/
</If>
<If "%{REQUEST_URI} =~ m#^/fileadmin/_processed_/|^/typo3temp/#">
    ExpiresActive On
    ExpiresDefault "access plus 1 year"
</If>
<If "%{REQUEST_URI} =~ m#^/typo3/sysext/|^/typo3conf/ext/#">
    ExpiresActive On
    ExpiresDefault "access plus 5 minutes"
</If>
# END OF Hn\Typo3Environment\Generator\HtaccessGenerator
